W			2.88;



Xmax			#calc "20*$W";
Xmin			#calc "-40*$W";
Xmid			#calc "$W";
L			#calc "-30*$W";

Ymax			#calc "2*$W";
Ymid			#calc "0.75*$W";
Ymid1			#calc "0.5*$W";

Z			0.01;

Z1			#calc "-$Z";

